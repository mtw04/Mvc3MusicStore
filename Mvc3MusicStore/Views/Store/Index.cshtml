@model IEnumerable<Mvc3MusicStore.Models.Genre>

@{
    ViewBag.Title = "Store";
}

<h3>Browse Genres</h3>
<p>
    Select from @Model.Count() genres:
</p>
<ul>
    @foreach (Mvc3MusicStore.Models.Genre genre in Model)
    {
        <li>
            @Html.ActionLink(genre.Name, "Browse", new { genre = genre.Name })
        </li>
    }
</ul>
